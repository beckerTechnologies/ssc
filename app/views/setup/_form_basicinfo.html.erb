  <div class="field">
    <p><%= f.label(:first_name, "First name") %>
    <%= f.text_field :first_name, placeholder: "Enter Your First Name" %> </p>
  </div>
  <div class="field">
    <p><%= f.label(:middle_name, "Middle name") %>
    <%= f.text_field :middle_name, placeholder: "Enter Your Middle Name" %> </p>
  </div>
  <div class="field">
    <p><%= f.label(:last_name, "Last name") %>
    <%= f.text_field :last_name, placeholder: "Enter Your Last Name" %> </p>
  </div>
  <div class="dob_select">
    <p><%= f.label(:dob, "Date of Birth") %>
    <!-- 
    <%= f.date_select :dob, start_year: 1900, end_year: 2014, order: [:year, :day, :month]%> </p> -->
    <%= f.text_field :dob,  placeholder: "Click to choose \"Date of Birth\""%>
  </div>
  <div class="field">
    <p><%= f.label(:ssn, "Social Security Number")%>
    <%= f.text_field :ssn, placeholder: "Enter SSN" %> </p>
  </div>
